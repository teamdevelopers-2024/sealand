{
  "name": "sea-land",
  "version": "1.0.0",
  "chromium-args": "--enable-node",
  "main": "frontend/dist/index.html",
"scripts": {
  "start": "nw .",
  "debug": "npx nw . --enable-logging=stderr",
  "build": "nwbuild --win --outDir build ."
}

,
  "window": {
    "title": "Sea Land",
    "width": 1024,
    "height": 768,
    "toolbar": true,
    "debug": true,
    "show_in_taskbar": true,
    "frame": true,
    "web_preferences": {
      "web_security": false
    }
  },
  "node-remote": "<all_urls>",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "nw": "^0.91.0",
    "nw-builder": "^4.11.0"
  }
}
